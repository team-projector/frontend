{
  "name": "team-projector",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "gulp": "gulp",
    "start": "npm run themes && ng serve",
    "build": "npm run themes && ng build --prod --localize",
    "build:mocks": "npm run themes && ng build --configuration=mocks --localize && ng build --configuration=ru",
    "test": "ng test",
    "lint": "ng lint",
    "clean": "npm cache clean --force && rm -rf -- node_modules doc coverage dist dll package-lock.json",
    "refresh": "npm run clean && npm install",
    "mocks": "tsc && gulp build --gulpfile node_modules/@junte/mocker-library/gulpfile.js --cwd .",
    "extract-i18n": "ng xi18n --output-path locale && xliffmerge --profile xliffmerge.json",
    "e2e": "ng e2e",
    "themes:watch": "gulp themes:watch",
    "themes": "gulp themes"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.0.0-rc.11",
    "@angular/cdk": "^8.2.3",
    "@angular/common": "^9.0.0-rc.11",
    "@angular/compiler": "^9.0.0-rc.11",
    "@angular/core": "^9.0.0-rc.11",
    "@angular/forms": "^9.0.0-rc.11",
    "@angular/localize": "^9.0.0-rc.11",
    "@angular/platform-browser": "^9.0.0-rc.11",
    "@angular/platform-browser-dynamic": "^9.0.0-rc.11",
    "@angular/router": "^9.0.0-rc.11",
    "apollo-angular": "^1.7.0",
    "apollo-angular-link-http": "^1.6.0",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-link-error": "^1.1.12",
    "apollo-link-schema": "^1.2.4",
    "core-js": "^3.6.4",
    "date-fns": "2.9.0",
    "faker": "^4.1.0",
    "gql-query-builder": "^3.1.3",
    "graphql": "^14.6.0",
    "graphql-tag": "^2.10.0",
    "graphql-tools": "^4.0.5",
    "jasmine": "^3.5.0",
    "junte-ui": "0.0.407",
    "ngx-cookie-service": "^2.3.0",
    "ngx-date-fns": "5.0.0",
    "rxjs": "^6.5.4",
    "serialize-ts": "2.5.4",
    "tslib": "^1.10.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.900.0-rc.9",
    "@angular/cli": "~9.0.0-rc.9",
    "@angular/compiler-cli": "^9.0.0-rc.11",
    "@angular/language-service": "^9.0.0-rc.11",
    "@types/jasmine": "~3.4.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "clean-deep": "^3.2.0",
    "codelyzer": "^5.2.1",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-hash": "^4.2.2",
    "gulp-sass": "^4.0.2",
    "jasmine-core": "~3.4.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.5.1",
    "protractor": "5.4.3",
    "reflect-metadata": "^0.1.13",
